{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "node": true,
    "amd": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "semi": [2, "never"],
    "no-console": 0
  }
}